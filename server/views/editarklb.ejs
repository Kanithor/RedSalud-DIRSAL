<% include header.ejs %>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <h2>Lista de Preguntas Personalizadas a Responder</h2>
      <thead class="thead-dark">
        <tr>
          <th scope="col-1">#</th>
          <th scope="col-2">Area</th>
          <th scope="col-2">Pregunta</th>
        </tr>
      </thead>
      <tbody>
        <% preguntas.forEach(function(item) { %>
        <tr>
            <td> ### </td>
            <td><%= item.area %> </td>
            <td><%= item.titulo %> </td>
        

        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

  <% include footer.ejs %>