<% include header.ejs %>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <h2>Lista de horas encoladas en base de datos</h2>
      <thead class="thead-dark">
        <tr>
          <th scope="col-1">#</th>
          <th scope="col-2">Nombre</th>
          <th scope="col-2">Rut</th>
          <th scope="col-2">E-mail</th>
          <th scope="col-5">Estado</th>
          <th scope="col">Afiliado</th>
          <th scope="col">Prevision</th>
        </tr>
      </thead>
      <tbody>
        <% pacientes.forEach(function(item) { %>
        <tr>
            <td> ### </td>
            <td><%= item.nombre %> </td>
            <td><%= item.rut %> </td>
            <td><%= item.email %> </td>
            <td><%= item.estado %> </td>
            <td><%= item.afiliado %> </td>
            <td><%= item.prevision %> </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

  <% include footer.ejs %>