<% include header.ejs %>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <h2>Lista de Usuarios</h2>
      <thead class="thead-dark">
        <tr>
          <th scope="col-1">#</th>
          <th scope="col-2">Nombre</th>
          <th scope="col-2">Rut</th>
          <th scope="col-2">Email</th>
          <th scope="col-2">Cargo</th>
          <th scope="col-5">Atributo</th>
          <th scope="col">Eliminar Usuario </th>
        </tr>
      </thead>
      <tbody>
        <% solicitudes.forEach(function(item) { %>
        <tr>
            <td> ### </td>
            <td><%= item.nombre %> </td>
            <td><%= item.rut %> </td>
            <td><%= item.email %></td>
            <td><%= item.tipo %> </td>
            <td><%= item.custom_attributes %> </td>
            <td>
                <button type="button" class="btn btn-info">Eliminar Usuario</button>
            </td>

        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

  <% include footer.ejs %>