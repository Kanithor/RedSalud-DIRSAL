<% include header.ejs %>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <h2>Lista de horas encoladas en base de datos</h2>
      <thead class="thead-dark">
        <tr>
          <th scope="col-1">#</th>
          <th scope="col-2">Paciente</th>
          <th scope="col-2">Fecha</th>
          <th scope="col-2">Area</th>
          <th scope="col-5">Comentarios</th>
        </tr>
      </thead>
      <tbody>
        <% solicitudes.forEach(function(item) { %>
        <tr>
            <td> ### </td>
            <td><%= item.paciente %> </td>
            <td><%= item.fecha %> </td>
            <td><%= item.area %> </td>
            <td><%= item.comentarios %> </td>

        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

  <% include footer.ejs %>