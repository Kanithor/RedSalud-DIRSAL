<% include header.ejs %>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <h2>Lista de Areas</h2>
      <thead class="thead-dark">
        <tr>
          <th scope="col-1">#</th>
          <th scope="col-2">Area</th>
        </tr>
      </thead>
      <tbody>
        <% solicitudes.forEach(function(item) { %>
        <tr>
            <td> ### </td>
            <td><%= item.nombre %> </td>
            <td>
                <button type="button" class="btn btn-info">Eliminar Area</button>
            </td>

        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>


  <% include footer.ejs %>